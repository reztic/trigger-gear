package edu.cs151.trigger.util;
import edu.cs151.trigger.entities.Entity;

/**
 * This interface is used by PlayerShip and Enemy and is implemented by GameManager.  This is
 * used to allow the PlayerShip and Enemy to notify the GameManager when it is firing its weapon
 * @author Philip Vaca
 *
 */
public interface ProjectileListener {
	/**
	 * When a projectile is fired, this method gets called
	 * @param e The projectile
	 */
	public void addProjectile(Entity e);
}
